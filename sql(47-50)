-- DDL for Question 47
CREATE TABLE networks (
    id INT PRIMARY KEY,
    cidr VARCHAR(255)
);

CREATE TABLE instances (
    network_id INT,
    cpu_usage VARCHAR(255),
    memory_usage VARCHAR(255),
    network_usage VARCHAR(255),
    FOREIGN KEY (network_id) REFERENCES networks(id)
);

