CREATE TABLE devices (
    id INT PRIMARY KEY,
    mac_address VARCHAR(255)
);

CREATE TABLE scanned_files (
    device_id INT REFERENCES devices(id),
    filename VARCHAR(255),
    is_infected SMALLINT
);

